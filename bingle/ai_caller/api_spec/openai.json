{
  "endpoint_uri": "https://api.openai.com/{resource_path}",
  "headers": {
    "Authorization": "Bearer {apikey}",
    "Content-Type": "application/json"
  },
  "resource_path": {
    "chat": "v1/chat/completions"
  },
  "default": {
    "chat": {
      "model": "gpt-4o-mini"
    }
  },
  "supported_models": [
    "gpt-4o-mini",
    "gpt-4o",
    "gpt-4-turbo",
    "o1-mini",
    "o1-preview",
    "gpt-3.5-turbo"
  ]
}
