{
  "endpoint_uri": "https://api.llama-api.com/{resource_path}",
  "headers": {
    "Authorization": "Bearer {apikey}",
    "Content-Type": "application/json"
  },
  "resource_path": {
    "chat": "chat/completions"
  },
  "default": {
    "chat": {
      "model": "llama3.2-3b"
    }
  },
  "supported_models": [
    "llama3.2-1b",
    "llama3.2-3b",
    "llama3.2-11b-vision",
    "llama3.2-90b-vision",
    "llama3.3-70b",
    "gemma2-9b",
    "gemma2-27b"
  ]
}
